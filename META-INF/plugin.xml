<idea-plugin version="2">
	<id>krasa.importedMavenProjectRenamer</id>
	<name>Imported Maven Project Renamer</name>
	<version>1.0</version>

	<vendor url="https://github.com/krasa/imported-maven-project-renamer-intellij-plugin" email="vojta.krasa@gmail.com">Vojtech
		Krasa
	</vendor>

	<description><![CDATA[
    ]]></description>

	<change-notes><![CDATA[
    ]]>
	</change-notes>

	<!-- please see http://confluence.jetbrains.com/display/IDEADEV/Build+Number+Ranges for description -->
	<idea-version since-build="139" />

	<!-- please see http://confluence.jetbrains.com/display/IDEADEV/Plugin+Compatibility+with+IntelliJ+Platform+Products
		   on how to target different products -->
	<!-- uncomment to enable plugin in all products
	  <depends>com.intellij.modules.lang</depends>
	  -->

	<extensions defaultExtensionNs="com.intellij">
		<projectImportProvider order="first" implementation="krasa.importedMavenProjectRenamer.MyMavenProjectImportProvider" />
		<projectOpenProcessor order="first" implementation="krasa.importedMavenProjectRenamer.MyMavenProjectOpenProcessor" />
		<projectImportBuilder order="first" implementation="krasa.importedMavenProjectRenamer.MyMavenProjectBuilder" />


		<!-- Add your extensions here -->
	</extensions>

	<application-components>
		<!-- Add your application components here -->
	</application-components>

	<project-components>
		<!-- Add your project components here -->
	</project-components>

	<actions>
		<!-- Add your actions here -->
	</actions>
	<depends>org.jetbrains.idea.maven</depends>

</idea-plugin>